{
  "index_name": "一条懒羊羊",
  "start_urls": ["http://www.jwblog.cn"],
  "rateLimit": 8,
  "maxDepth": 10,
  "selectors": {
    "lvl0": {
      "selector": ".cb-title",
      "defaultValue": "Documentation"
    },
    "lvl1": ".container h1",
    "lvl2": ".container h2",
    "lvl3": ".container h3",
    "lvl4": ".container h4",
    "lvl5": ".container h5",
    "content": ".container p, .container li",
    "lang": {
      "selector": "/html/@lang",
      "type": "xpath",
      "global": true
    }
  },
  "selectors_exclude": [
    "aside",
    ".page-footer",
    ".next-and-prev-link",
    ".table-of-contents"
  ],
  "custom_settings": {
    "attributesForFaceting": ["lang", "tags"]
  },
  "js_render": true
}
